<div class="container">
  <div class="row">
    <% @groups.find_each do |group| %>
      <div class="col-md-4">
        <div class="card">
          <div class="card-header"><%= group %></div>
          <% group.landings.find_each do |landing| %>
            <div class="card-body">
              <%= link_to landing.name, landing.url, class: "" %><br>
              <%= link_to landing.url, landing.url, class: "" %><br>
              <%= link_to "edit", edit_landing_path(landing), class: "" %>
              <%= link_to 'remove', landing, method: :delete, data: { confirm: 'Are you sure?' }, class: "" %>
              <%= icon('fas', 'flag') %>
              <%= "retro" %>
            </div>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>
<% console %>
